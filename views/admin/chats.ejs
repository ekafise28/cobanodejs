<%- include('../_layouts/adminheader') %>


<div class="ibox-content col-lg-8 mx-auto">
<div class="jumbotron" style="text-align:center;margin-bottom: 10px;">
        <h1><b>Chatting</b></h1>  
  </div>

<table class="table table-striped alignmiddle">

 <thead  style='background-color:#FF4500; color:#FFF;'>
    <tr class="home">
      <th>Iklan</th>
      <th>Pesan</th>
      <th>Tanggal</th>
      
    </tr>
  </thead>
  <tbody>
    <% chats.forEach(function(chats){ %>
    <tr>
      <td>
        <%= chats.product %>
      </td>
      <td>
        <%= chats.message %>
      </td>
      <td>
        <%= chats.sendtime %>
      </td>
    </tr>
    <% }); %>
  </tbody>
</table>


<%- include('../_layouts/adminfooter') %>